@page
@model MyPhotosASP.Pages.Persons.ListModel
@{
    ViewData["Title"] = "List";
}

<h1>List persons:</h1>

<table class="table">
<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Persons[0].Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Persons[0].personName)
        </th>
    </tr>
</thead>
<tbody>
    @foreach (var item in Model.Persons)
    {
        <tr>
            <td>
                @Html.DisplayFor(model => item.Id)
            </td>
            <td>
                @Html.DisplayFor(model => item.personName)
            </td>
        </tr>
    }
</tbody>
</table>
<div>
    <a asp-page="/Medias/Index">Back to List</a>
</div>